{
  "name": "renaissance-styleguide",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github-limecodes:limecodes/renaissance-styleguide.git",
  "author": "limecodes <yan.shcherbakov+limecodes@gmail.com>",
  "license": "Private",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1"
  },
  "scripts": {
    "format": "prettier . --write"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
